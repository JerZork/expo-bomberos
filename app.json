{
  "expo": {
    "name": "pruebatailwind",
    "slug": "pruebatailwind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_sistema.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jpalma1414.pruebatailwind"
    },
    "plugins": [
      "./plugins/withCleartext.js"
    ],
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "a3510e50-4548-42f4-b0ef-812cecdb1916"
      }
    }
  }
}
